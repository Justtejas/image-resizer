{
  "name": "image-resizer",
  "version": "1.0.0",
  "productName": "Image Resizer",
  "description": "image resizer",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "packager-win": "electron-packager . --platform=win32 --arch=x64 --icon=assets/icons/win/icon.ico --out=release-builds --overwrite --asar",
    "packager-linux": "electron-packager . --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --out=release-builds --overwrite --asar",
    "packager-mac": "electron-packager . --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --out=release-builds --overwrite --asar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Justtejas/image-resizer.git"
  },
  "keywords": [
    "image",
    "resizer"
  ],
  "author": "Tejas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Justtejas/image-resizer/issues"
  },
  "homepage": "https://github.com/Justtejas/image-resizer#readme",
  "dependencies": {
    "resize-img": "^2.0.0",
    "toastify-js": "^1.12.0"
  },
  "devDependencies": {
    "@electron/packager": "^18.1.3",
    "electron": "^28.1.4"
  }
}
